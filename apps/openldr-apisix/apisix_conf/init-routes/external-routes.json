[
  {
    "id": "data-processing-route",
    "uri": "/api/v1/openldr/event/msg",
    "name": "Data processing route",
    "methods": ["POST", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/event/msg", "/data-processing/send-message"],
    "upstream_url": "${DATA_PROCESSING_URL}"
  },
  {
    "id": "user-datafeed-authentication-route",
    "uri": "/api/v1/openldr/auth/token",
    "name": "User and data feed authentication route",
    "methods": ["POST", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/auth/token", "/realms/openldr-realm/protocol/openid-connect/token"],
    "upstream_url": "${KEYCLOAK_URL}",
    "extra_upstream": {
      "scheme": "http",
      "pass_host": "pass",
      "hash_on": "vars"
    }
  }
]