[
  {
    "id": "project-creation-route",
    "uri": "/api/v1/openldr/project/create",
    "name": "Project creation route",
    "methods": ["POST", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/project/create", "/project/create-project"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "project-get-all-route",
    "uri": "/api/v1/openldr/project",
    "name": "Project get all route",
    "methods": ["GET", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/project", "/project"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "project-update-route",
    "uri": "/api/v1/openldr/project/update/*",
    "name": "Project update route",
    "methods": ["PUT", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/project/update/(.*)", "/project/update-project/$1"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "project-delete-route",
    "uri": "/api/v1/openldr/project/delete/*",
    "name": "Project delete route",
    "methods": ["DELETE", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/project/delete/(.*)", "/project/delete-project/$1"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  }
