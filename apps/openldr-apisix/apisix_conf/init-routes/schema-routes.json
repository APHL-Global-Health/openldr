[
    
  {
    "id": "schema-get-all-route",
    "uri": "/api/v1/openldr/schema",
    "name": "Schema get all route",
    "methods": ["GET", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/schema", "/schema"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "schema-retrieval-route",
    "uri": "/api/v1/openldr/schema/retrieve/*",
    "name": "Schema retrieval route",
    "methods": ["GET", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/schema/retrieve/(.*?)/(.*)", "/schema/get-schema/$1/$2"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  }
]