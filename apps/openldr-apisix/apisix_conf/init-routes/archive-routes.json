[
  {
    "id": "archive-get-data-route",
    "uri": "/api/v1/openldr/archive/data/*",
    "name": "Get all data route",
    "methods": ["POST", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/archive/data/(.*)", "/archive/data/$1"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "archive-columns-route",
    "uri": "/api/v1/openldr/archive/table/*",
    "name": "Retrieval table columns route",
    "methods": ["GET", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/archive/table/(.*)", "/archive/table/$1"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "archive-multiple-route",
    "uri": "/api/v1/openldr/archive/table/*",
    "name": "Table data routes",
    "methods": ["POST","DELETE","PUT", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/archive/table/(.*?)/(.*?)/(.*)", "/archive/table/$1/$2/$3"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  },
  {
    "id": "archive-tables-route",
    "uri": "/api/v1/openldr/archive/tables",
    "name": "Get all tables route",
    "methods": ["GET", "OPTIONS"],
    "rewrite_uri": ["/api/v1/openldr/archive/tables", "/archive/tables"],
    "upstream_url": "${ENTITY_SERVICE_URL}"
  }
]