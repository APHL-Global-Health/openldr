{
  "name": "@openldr/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy:env": "cpy ../../.env.web . --rename=.env",
    "copy:env:dev": "cpy ../../.env.web . --rename=.env.development",
    "copy:env:prod": "cpy ../../.env.web . --rename=.env.production",
    "copy:env:staging": "cpy ../../.env.web . --rename=.env.staging",
    "copy:env:all": "npm run copy:env && npm run copy:env:dev && npm run copy:env:prod && npm run copy:env:staging",
    "docker:build": "npm run copy:env:all && tsx ./docker-compose.ts build",
    "docker:start": "npm run copy:env:all && tsx ./docker-compose.ts up -d",
    "docker:stop": "npm run copy:env:all && tsx ./docker-compose.ts stop",
    "docker:reset": "docker compose -f docker-compose.yml down --rmi all -v --remove-orphans && rimraf ./.env*",
    "dev": "npm run copy:env:all && cross-env NODE_ENV=development vite",
    "build": "tsc -b && vite build --mode production",
    "start": "cross-env NODE_ENV=production node server.mjs",
    "lint": "eslint .",
    "monitor": "cross-env NODE_ENV=development nodemon server.mjs",
    "preview": "vite preview",
    "wasm": "cpy node_modules/@sqlite.org/sqlite-wasm/sqlite-wasm/jswasm/sqlite3.wasm public/"
  },
